<!doctype html>
<html>
    <head>
        <title>{+pageTitle}Google{/pageTitle}</title>
        <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
        <meta content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0" name="viewport">
        <meta name="theme-color" content="{+theme}#ffffff{/theme}" />
        <link rel="stylesheet" href="base.css" />
        {?style}
            <link rel="stylesheet" href="{style}" />
        {/style}
        <script type="text/javascript" src="script.js" defer="defer"></script>
    </head>
    <body>
        <div class="page">
            {+content /}
            <div class="location">
                <span class="dot"></span>The Woods
            </div>
        </div>
        <footer>
            <ul class="footer-links">
                <li>Settings</li>
                <li>Privacy</li>
                <li>Terms</li>
            </ul>
            <ul class="footer-links">
                <li>Advertising</li>
                <li>Business</li>
                <li>About</li>
            </ul>
        </footer>
    </body>
</html>